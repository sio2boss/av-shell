#! /bin/bash

# Import color
source $AV_CONFIG_DIR/default_imports

# Check for a help flag
avCheckHelpFlags "$1" "boot2docker" "Get boot2docker ip address" "\n \
Basically runs boot2docker ip\n \
\n"

# Import system path
avRestoreSystemPath

#### Put system commands here
ip=$(boot2docker ip)
echo -n "${ip}"
#### End commands
