#! /bin/bash

# Import color
source $AV_CONFIG_DIR/default_imports

# Check for a help flag
avCheckHelpFlagsPassThrough "$1" "docker" "Run a command \
in a running container" "docker exec --help"

# Import system path
avRestoreSystemPath

#### Put system commands here
cd $AV_ROOT/.. && docker exec $*
#### End commands
