#! /bin/bash


# Import color
source $AV_CONFIG_DIR/default_imports

# Check for a help flag
avCheckHelpFlags "$1" "git" "Shortcut for rebase to master" "\n \
    Just runs git rebase master.\n \
\n"

# Import system path
avRestoreSystemPath

#### Put system commands here

cd $AV_ROOT/.. && git rebase master

#### End commands
