#! /bin/bash


# Import color
source $AV_CONFIG_DIR/default_imports

# Check for a help flag
avCheckHelpFlags "$1" "builtin" "Get persistent variable" "\n \
    Template to help you out.\\n \
\n"


#### Put system commands here

# Get variable stored on disks
/bin/cat $AV_CONFIG_DIR/vars/$1

#### End commands
