#! /bin/bash


# Import color
source $AV_CONFIG_DIR/default_imports

# Check for a help flag
avCheckHelpFlags "$1" "builtin" "Change the name of a script" "\n \
This runs mv 1 -> 2.  And should include the subdirectory as in:\n \
\n \
  example: rename builtin/foo builtin/magic\n \
\n"

# Import system path
avRestoreSystemPath

#### Put system commands here

if [ $# -ne 2 ]; then
  echo "usage: rename <current> <new>"
  echo "example: rename foo bar"
  echo "example: rename builtin/foo builtin/magic"
  exit
fi

mv $1 $2

#### End commands
