#! /bin/bash


# Import color
source $AV_CONFIG_DIR/default_imports
source $AV_CONFIG_DIR/findjar

# Check for a help flag
avCheckHelpFlags "$1" "av-gcp" "Access to gcloud" "\n \
    Runs GCP commands via gcloud \n \
\n"

# Import system path
avRestoreSystemPath

#### Put system commands here

cd $AV_ROOT/.. && gcloud $*

#### End commands
